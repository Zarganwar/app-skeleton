{
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "php-di/php-di": "7.0.6",
    "psr/container": "^2.0",
    "psr/http-message": "^1.1",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^3.0",
    "ramsey/uuid": "^4.7",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "zarganwar/command-bus": "^1.1"
  },
  "require-dev": {
    "nette/tester": "^2.5",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "phpstan": "phpstan analyse -c phpstan.neon --ansi",
    "test": "vendor/bin/tester tests"
  }
}
